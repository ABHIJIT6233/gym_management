<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gymfemale"
    tools:context=".femaleplan">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="20sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:background="#FFEB3B"
        android:gravity="center"
        android:textSize="20sp"
        android:textColor="#03A9F4"
        android:text="FEMALE PLANS" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="20sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#00BCD4"
        android:text="1 month : RS 500" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#00BCD4"
        android:layout_marginBottom="20sp"
        android:text="6 months : RS 1200" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="20sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#00BCD4"
        android:text="1 year : RS 2200" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="20sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:background="#03A9F4"
        android:layout_marginTop="10sp"
        android:gravity="center"
        android:textSize="20sp"
        android:textColor="#FFEB3B"
        android:text="LIST OF TRAINERS " />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="10sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#FF9800"
        android:text="Mr.Rajesh Patil (Male Trainer)" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="10sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#FF9800"
        android:text="Miss.Deepali Sharma (Female Trainer)" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:layout_marginBottom="10sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#FF9800"
        android:text="Mr.Indrajeet Singh (Male Trainer and Owner)" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10sp"
        android:textColor="@color/white"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="60sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:background="#FF9800"
        android:layout_marginBottom="10sp"
        android:text="Miss.Ria Dubey (Female Trainer and physiotherapist)" />


    <Button
        android:id="@+id/back"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="80sp"
        android:layout_marginRight="80sp"
        android:text="back" />

    <Button
        android:id="@+id/next"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="80sp"
        android:layout_marginRight="80sp"
        android:layout_marginTop="10sp"
        android:text="next  " />



</LinearLayout>